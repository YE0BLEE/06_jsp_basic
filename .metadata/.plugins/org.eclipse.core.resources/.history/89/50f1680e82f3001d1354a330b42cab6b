package com.lec.board;

public class Board {
	
	private int bno;
	private String subject;
	private String writer;
	private String crtdate;
	private int readcnt;
	private String content;
	
	public Board() {
		this.bno = 0;
		this.subject = null;
		this.writer = null;
		this.crtdate = null;
		this.readcnt = 0;
		this.content = null;
	}
	
	

}
